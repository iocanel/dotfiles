# iocanel@gmail.com
account        iocanel@gmail.com
host           smtp.gmail.com
port           587
from           iocanel@gmail.com
user           iocanel
passwordeval   "pass show mail.google.com/iocanel@gmail.com/token"
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
auth on
logfile ~/.local/state/msmtp/personal.log
